{
  "name": "meu-portal-rommanel-mfe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "dev": "SET NEXT_PRIVATE_LOCAL_WEBPACK=true && next dev -p 3006",
    "build": "SET NEXT_PRIVATE_LOCAL_WEBPACK=true && node envs.js && next build",
    "format": "prettier '**/*.{ts,tsx}' --write",
    "start": "SET NEXT_PRIVATE_LOCAL_WEBPACK=true && export NODE_ENV=production && next start -p 3006",
    "deploy": "bash deploy.bash",
    "lint": "next lint",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "test": "jest ",
    "test:ci": "jest --ci",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/styled": "^11.10.0",
    "@module-federation/nextjs-mf": "8.8.8",
    "next": "14.2.25",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.5",
    "prettier-eslint": "12.0.0",
    "prettier-eslint-cli": "5.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "webpack": "5.97.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@emotion/react": "^11.10.0",
    "@next/third-parties": "14.1.4",
    "@tanstack/react-query": "5.29.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/testing-library__jest-dom": "^5.14.6",
    "commitizen": "4.3.1",
    "cypress": "13.9.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "esbuild": "^0.15.16",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.25",
    "eslint-config-prettier": "8.5.0",
    "git-cz": "4.8.0",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-sonar": "^0.2.16",
    "standard-version": "^9.5.0",
    "styled-components": "6.1.13",
    "@types/styled-components": "^5.1.34",
    "babel-plugin-styled-components": "^2.1.4",
    "typescript": "5.4.4"
  },
  "resolutions": {
    "cross-spawn": "7.0.6",
    "ws": "8.17.1",
    "semver": "7.5.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
