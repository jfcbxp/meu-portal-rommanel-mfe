{
  "name": "meu-portal-rommanel-mfe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "dev": "SET NEXT_PRIVATE_LOCAL_WEBPACK=true && next dev -p 3006",
    "build": "SET NEXT_PRIVATE_LOCAL_WEBPACK=true && node envs.js && next build",
    "format": "prettier 'src/app/**/*.{ts,tsx,json}' --write",
    "start": "SET NEXT_PRIVATE_LOCAL_WEBPACK=true && export NODE_ENV=production && next start -p 3006",
    "deploy": "bash deploy.bash",
    "lint": "next lint",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "test": "jest ",
    "test:ci": "jest --ci",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "prettier-eslint": "16.4.1",
    "prettier-eslint-cli": "8.0.1",
    "@emotion/styled": "^11.14.0",
    "date-fns": "^3.6.0",
    "next": "15.3.1",
    "webpack": "5.99.7"
  },
  "devDependencies": {
    "@tanstack/react-query": "5.74.11",
    "@next/third-parties": "15.3.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@emotion/react": "^11.10.0",
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "commitizen": "4.3.1",
    "esbuild": "^0.25.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "10.1.2",
    "git-cz": "4.9.0",
    "husky": "^9.1.7",
    "styled-components": "6.1.17",
    "@eslint/eslintrc": "^3"
  }
}
